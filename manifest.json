{
  "manifest_version": 2,

  "name": "Hey Jenny!",
  "permissions": [ "cookies", "tabs", "background", "http://*/*", "https://*/*" ],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "description": "Hey Jenny! Your personal video voice controller...",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "jenny.html"
  }
}